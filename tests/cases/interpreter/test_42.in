// This program should print a different string based on the value of age
var stage = "unknown";
var age = 86;
if (age < 18) { stage = "child"; }
if (age >= 18) { stage = "adult"; }
print stage;
var isAdult = age >= 18;
if (isAdult) { print "eligible for voting: true"; }
if (!isAdult) { print "eligible for voting: false"; }
